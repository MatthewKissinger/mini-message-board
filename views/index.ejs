<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <a href="http://localhost:3000/new">New Message</a>
    
      <% messages.forEach(function(message) { %>
        <div class="message-div">
          <h1><%= message.user %></h1>
          <h2><%= message.text %></h2>
          <h3><%= message.added %></h3>
        </div>
      <% }); %>
     
  </body>
</html>
